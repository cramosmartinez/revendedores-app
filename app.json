{
  "expo": {
    "name": "revendedores-app",
    "slug": "revendedores-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/revendedores.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/revendedores.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carlosramos.ventaspro" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/revendedores.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.carlosramos.ventaspro",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/images/revendedores.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir acceso para escanear productos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "79055c60-5e30-430c-ae2e-010505efd479" 
      }
    }
  }
}